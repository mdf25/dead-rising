<configs>
	<!-- Adds a dependency tag to the start of the XML -->
	<insertBefore xpath="/*/*[1]">
		<!-- DEPENDENCY: FennecModlet - Foods - Additional Foods -->
	</insertBefore>
	
	<!-- Adds the crop seeds in this mod to the 'Living Off The Land' perk. -->
	<append xpath="//perk[@name='perkLivingOffTheLand']/effect_group/passive_effect[@name='RecipeTagUnlocked' and @level='1,5']/@tags">,treePlantedApple0FM,treePlantedPear0FM,plantedSoybean1FM,plantedTomato1FM,plantedWheat1FM</append>
	
	<!-- Adds the different food recipes from this mod into the 'Master Chef' perk. -->
