			<property name="CustomIconTint"		value="ff0000" />
			<property name="DescriptionKey"		value="adminArenaBlocksDesc" />
			<property name="Material" 			value="Mbedrock" />
			<property name="Texture" 			value="241" />
			<property name="CreativeMode" 		value="None" />
			<property name="FilterTags" 		value="fbuilding,fadmin" />
			<property name="SortOrder1" 		value="4020" />
			<property name="SortOrder2" 		value="0002" />
			<property name="PassThroughDamage" 	value="true" />
			<drop event="Destroy"	 			count="0"/>
			<property class="UpgradeBlock">
				<property name="ToBlock" 		 	value="woodFrameBlock" />
				<property name="Item" 			 	value="resourceAdminBlockRemover" />
				<property name="ItemCount" 		 	value="1" />
